        <action name="administracion">
            <result>/administracion/administracion.jsp</result>
        </action>
        
        <action name="OperacionesAdministracionAction" class="administracion.actions.OperacionesAdministracionAction">
            <result name="USUARIO">/administracion/usuario/usuario.jsp</result>            
