            <result name="CATEGORIA">/administracion/genero/categoria.jsp</result>
            <result name="PRODUCTO">/administracion/genero/producto.jsp</result>                          
        </action>
          
        <action name="OperacionesUsuarioAction" class="administracion.actions.OperacionesUsuarioAction">
        </action> 
         
        <action name="OperacionesDatosRestauranteAction" class="administracion.actions.OperacionesDatosRestauranteAction">
